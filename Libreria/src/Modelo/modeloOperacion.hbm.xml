<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="Modelo.modeloOperacion" table="operacion" >
   <id column="id_operacion" name="id_operacion" type="integer">
       <generator class="native"/>
   </id>
   <property column="libro_id_libro" name="libro_id_libro" type="integer"/>
   <property column="usuarios_id_usuario" name="usuarios_id_usuario" type="integer"/>
   <property column="tipo_op" name="tipo_op"  type="string"   />
   <property column="fecha_recibido" name="fecha_recibido"   type="date"/>
  </class>
</hibernate-mapping>
